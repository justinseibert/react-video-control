{
  "name": "@justinseibert/react-video-control",
  "description": "Video timeline controller built with canvas and d3 for better touch performance",
  "author": "Justin Seibert",
  "keywords": [
    "react",
    "components",
    "ui",
    "slider",
    "video"
  ],
  "version": "0.1.0",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justinseibert/react-video-control.git"
  },
  "dependencies": {
    "d3-drag": "^3.0.0",
    "d3-selection": "^3.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "build": "rm -rf dist && NODE_ENV=production babel src/component --out-dir dist --copy-files",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@types/d3-drag": "^3.0.1",
    "@types/d3-selection": "^3.0.2",
    "@types/node": "^16.11.36",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-scripts": "^5.0.1",
    "typescript": "^4.6.4"
  }
}
